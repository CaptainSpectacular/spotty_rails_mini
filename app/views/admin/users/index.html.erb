<h1 class="header page-header">
  All Users
</h1>

<div class="links">
  <%= link_to 'Create New User', new_admin_user_path, class: 'user-links admin-create' %>

  <% users.each do |user| %>
    <div class="user-index">
      <%= link_to user.username, edit_admin_user_path(user), class: 'user-links' %>: 
      <%= user.role %>
    </div>
  <% end %>
</div>
