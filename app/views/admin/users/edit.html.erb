<h1>Admin::Users#show</h1>
<p>Find me in app/views/admin/users/show.html.erb</p>
<%= user.username %>
<%= user.role %>
<%= user.created_at %>

<% unless user.admin? %>
  <%= link_to 'Delete', admin_user_path(user), method: :delete %>
  <%= link_to 'Promote', admin_user_path(user), method: :patch %>
<% end %>